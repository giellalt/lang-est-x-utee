3:kolm LÅGEVT ;
4:neli LÅGEVT ;
5:viis LÅGEVT ;
6:kuus LÅGEVT ;
7:seitse LÅGEVT ;
8:kaheksa LÅGEVT ;
9:üheksa LÅGEVT ;

LEXICON TENT
1%0:kümme% tuhat THOUSAND ;


LEXICON LUHKIET
:%0 THOUSANDS ;
1:akte THOUSANDS ;
ONEST ;

LEXICON ONEST
2:kaks THOUSANDS ;
3:kolm THOUSANDS ;
4:neli THOUSANDS ;
5:viis THOUSANDS ;
6:kuus THOUSANDS ;
7:seitse THOUSANDS ;
8:kaheksa THOUSANDS ;
9:üheksa THOUSANDS ;

LEXICON THOUSANDS      ! x > 1
tuhat: THOUSAND ;

LEXICON THOUSAND
UNDERTHOUSAND ;	   ! 1100-1999
%0: TENS ;	   ! 1020-1099
%0: TEENS ;	   ! 1010-1019
%0%0: ONES ;	   ! 1001-1009
