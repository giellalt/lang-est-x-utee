kolm:3 LÅGEVT ;
neli:4 LÅGEVT ;
viis:5 LÅGEVT ;
kuus:6 LÅGEVT ;
seitse:7 LÅGEVT ;
kaheksa:8 LÅGEVT ;
üheksa:9 LÅGEVT ;

LEXICON TENT
kümme tuhat:1%0 THOUSAND ;


LEXICON LUHKIET
:%0 THOUSANDS ;
akte:1 THOUSANDS ;
ONEST ;

LEXICON ONEST
kaks:2 THOUSANDS ;
kolm:3 THOUSANDS ;
neli:4 THOUSANDS ;
viis:5 THOUSANDS ;
kuus:6 THOUSANDS ;
seitse:7 THOUSANDS ;
kaheksa:8 THOUSANDS ;
üheksa:9 THOUSANDS ;

LEXICON THOUSANDS      ! x > 1
tuhat: THOUSAND ;

LEXICON THOUSAND
UNDERTHOUSAND ;	   ! 1100-1999
:%0 TENS ;	   ! 1020-1099
:%0 TEENS ;	   ! 1010-1019
:%0%0 ONES ;	   ! 1001-1009
:%0%0%0 COMMA ;	   ! 1000

!===========================
!Here starts the 999 numbers
!===========================

LEXICON UNDERTHOUSAND
HUNDREDS ;
sada:1 HUNDRED ;

LEXICON HUNDREDS
kaks:2   CUODI ;
kolm:3    CUODI ;
neli:4 CUODI ;
viis:5   CUODI ;
kuus:6   CUODI ;
seitse:7 CUODI ;
kaheksa:8 CUODI ;
üheksa:9   CUODI ;

LEXICON CUODI
sada:   HUNDRED ;

LEXICON HUNDRED
      TENS ;
      TEENS ;
:%0   ONES ;
:%0%0 COMMA ;

LEXICON CUODAAT
:%0%0%.    COMMA ;

LEXICON TEENS
:1 TEEN ;

LEXICON TEEN
