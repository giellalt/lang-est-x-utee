# II SUBJEKTIGRAMMATIKA

# 1) Verbi vorm välistab subjekti olemasolu 
# subjekti ei saa olla, kui selles osalauses on öeldis impersonaalis
REMOVE (@SUBJ) (*-1C Imps BARRIER CLBC OR Sid|Kvm LINK 0C Oeldis) ;
REMOVE (@SUBJ) (*1C Imps BARRIER CLBC OR Sid|Kvm LINK 0C Oeldis) ;

# 2) Sõna ei ühildu öeldisega
# Erand: eks _me_ tee neid tükke vahel nüüdki
SELECT Ppers + (@SUBJ) + Nom (*1 Imper + Vps2sg BARRIER CLB)
                (*-1 ("eks") BARRIER CLB) ;

# kui osalause öeldiseks on verb 1. või 2. pöördes, siis subjektiks
# saavad olla ainult isikulised asesõnad (vastavalt verbi vormile
# "mina", "sina", "meie", "teie"), kõik muu subjektiks ei ole

REMOVE (@SUBJ) (NEGATE 0 ("mina") OR ("ma"))(*-1C Vps1sg BARRIER Z OR CLBC) ;
REMOVE (@SUBJ) (NEGATE 0 ("mina") OR ("ma"))(*1C Vps1sg BARRIER Z OR CLBC) ;

REMOVE (@SUBJ) (NEGATE 0 ("sina") OR ("sa"))(*-1C Vps2sg BARRIER Z OR CLBC) ;
REMOVE (@SUBJ) (NEGATE 0 ("sina") OR ("sa"))(*1C Vps2sg BARRIER Z OR CLBC) ;
# Viga tekkis: eks nad varsti tee jälle süsti

REMOVE (@SUBJ) (NEGATE 0 Pps1 OR ("kõik"))
               (*1C Vps1pl BARRIER Sid|Kvm OR CLBC) ;
REMOVE (@SUBJ) (NEGATE 0 Pps1 OR ("kõik"))
               (*-1C Vps1pl BARRIER Sid|Kvm OR CLBC) ;
REMOVE (@SUBJ) (NEGATE 0 Pps2)(*1C Vps2pl BARRIER Sid|Kvm OR CLBC) ;
REMOVE (@SUBJ) (NEGATE 0 Pps2)(*-1C Vps2pl BARRIER Sid|Kvm OR CLBC) ;

# Santiiv või pronoomen mitmuse nominatiivis ei saa olla subjektiks,
# kui osalauses leidub verb ainsuses
REMOVE S + (@SUBJ) + PlNom OR P + (@SUBJ) + PlNom
               (*-1 Sg + FinV BARRIER Sid|Kvm OR CLBC) ;
REMOVE S + (@SUBJ) + PlNom OR P + (@SUBJ) + PlNom
               (*1 Sg + FinV BARRIER Sid|Kvm OR CLBC) ;
REMOVE SgNom + (@SUBJ) (*-1 PlVerb + FinV BARRIER Sid|Kvm OR CLBC LINK NOT 0 SgVerb)
               (NEGATE *1 (@<Q) BARRIER CLBC)(NEGATE 1 Sid|Kvm) ; # TP +NOT0sg
REMOVE SgNom + (@SUBJ) (*-1 PlVerb + FinV BARRIER Sid|Kvm OR CLBC)
               (NEGATE *1 (@<Q) BARRIER CLBC)(1 Koma)(NEGATE *1 S BARRIER CLBC) ;

# sõna partitiivis ei ole subjekt, kui osalauses leidub verb mitmuses
REMOVE Par + (@SUBJ) (*-1C PlVerb BARRIER CLBC)  ;
REMOVE Par + (@SUBJ) (*1C PlVerb BARRIER CLBC)  ;
# osastavas käändes nimi-asesona ei ole subjekt, kui osalause verb on esimeses voi teises pöördes.
REMOVE Par + (@SUBJ) (*-1C Vps1sg BARRIER CLB) ;
REMOVE Par + (@SUBJ) (*-1C Vps1pl BARRIER CLB) ;
REMOVE Par + (@SUBJ) (*-1C Vps2sg BARRIER CLB) ;
REMOVE Par + (@SUBJ) (*-1C Vps1pl BARRIER CLB) ;
# infinitiivi vorm saab olla subjekt üksnes siis, kui öeldiseks on verb
# 3. pöörde mitmuses, seega infinitiivi vorm ei ole subjekt, kui osalauses
# leidub verbe 1., 2. pöördes või 3. pöörde mitmuses
REMOVE Inf + (@SUBJ) (*-1C Vps1 OR Vps2 OR Vps3pl BARRIER CLBC) ;
REMOVE Inf + (@SUBJ) (*1C Vps1 OR Vps2 OR Vps3pl BARRIER CLBC) ;

# kui "mina", "meie", "teie" ühildub öeldisega, siis ta on alus
SELECT ("mina" @SUBJ) + SgNom OR ("ma" @SUBJ) + SgNom (*-1 Vps1sg BARRIER CLB) ;
SELECT ("mina" @SUBJ) + SgNom OR ("ma" @SUBJ) + SgNom (*1 Vps1sg BARRIER CLB) ;
SELECT ("mina" @SUBJ) + PlNom OR ("ma" @SUBJ) + PlNom (*-1 Vps1pl BARRIER CLB) ;
SELECT ("mina" @SUBJ) + PlNom OR ("ma" @SUBJ) + PlNom (*1 Vps1pl BARRIER CLB) ;
SELECT ("sina" @SUBJ) + PlNom OR ("sa" @SUBJ) + PlNom (*-1 Vps2pl BARRIER CLB) ;
SELECT ("sina" @SUBJ) + PlNom OR ("sa" @SUBJ) + PlNom (*1 Vps2pl BARRIER CLB) ;

# 3) Unikaalsus
# kui osalauses leidub ühene subjekt, siis rohkem subjekte olla ei saa,
# välja Natud koordineeritud subjektid
REMOVE (@SUBJ) (*-1C Subjekt BARRIER Sid|Kvm OR CLBC)(NEGATE 0 Nimi) ;
# Vead tekivad selletõttu, et kuskil midagi valesti määratud.
# Töömees on _eestlane_ küll, ...
# nad hoidsid teda ka nagu minu _vanemad_ mind
REMOVE (@SUBJ) (*1C Subjekt BARRIER Sid|Kvm OR CLBC)(NEGATE 0 Nimi) ;
REMOVE (@SUBJ) (*-1C Subjekt BARRIER CLB LINK *1 FinV BARRIER Sid|Kvm) ;
# Langend Patroklos ja keeb tema ümber nüüd metsikuim _võitlus_
REMOVE (@SUBJ) (*1C Subjekt BARRIER CLBC LINK *-1 FinV BARRIER Sid|Kvm) ;

# 4) Lause ehitusest soltuvad reeglid

# kui kõrvuti paiknevad kaks nime nominatiivis ja mõlemad võivad olla
# subjektiks, siis esimene ei ole subjekt
REMOVE (@SUBJ) (0 Nom)(1C Sprop + Nom)(1 Subjekt)(NEGATE 0 Pindef OR Ppers) ;
# Santiiv või pronoomen partitiivi mitmuses on subjekt, kui osalauses
# pole muud ühest subjekti ja öeldiseks on verb "olema"
SELECT SP + PlPar + (@SUBJ) (*-1 Olema BARRIER Verb OR CLB)
               (NEGATE *1 Verb BARRIER CLB)
               (NEGATE *-1 Subjekt BARRIER CLB)
               (NEGATE *1C Subjekt BARRIER CLB)
               (NEGATE *-1 NimKv OR KvAdv OR ("üks") BARRIER CLB) ;
# Viga tekkis enne: et on _teisigi_ Partei vaenlasi
# TP: oli: (NEGATE *-1C Subjekt BARRIER CLB)

# sõna partitiivis pole subjekt, kui talle järgneb da-infinitiivne öeldis
REMOVE Par + (@SUBJ) (1 Inf)(1C Oeldis) ; # TP Par viidud Subj juurde, oli 0-ting
# kui kõrvuti on nominatiivis ase- ja nimisõna, siis asesõna ei ole subjekt
REMOVE P + (@SUBJ) + SgNom (1 S + SgNom)(NEGATE -1 Olema)(NEGATE 0 Ppers) ;
REMOVE P + (@SUBJ) + SgNom (1 S + SgNom)(-1 Olema)
               (*1 NudTud BARRIER CLBC)(NEGATE 0 Ppers) ; 
# Kuid praegusel hetkel oli _see_ monoloogki katkenud.
# Aga: Veel enam aga oli see enesehüpnoos.
# Aga: Kui _sa_ siga alla ei anna, siis ..

# sõna partitiivis pole subjekt, kui öeldis on sellest sõnast paremal ja
# öeldisest paremal osalauses leidub nimi- või asesõna nominatiivis
# Kombinatsioon SubjPar Fv Obj(nom,gen) esineb va"ga hNa
REMOVE Par + (@SUBJ) (NEGATE *-1 Oeldis BARRIER CLB)
               (*1C Oeldis BARRIER Z OR CLB)
               (*1 SPNom BARRIER CLB LINK NOT 0 Aeg) ;

# da-infinitiivid
SELECT Inf + (@SUBJ) (NEGATE *-1 Inf BARRIER CLB)
           (NEGATE *1 Inf BARRIER CLB)
           (*-1 Suutelisusv OR Emotsioonv OR Sobivusv BARRIER CLB LINK 0C Oeldis) ;
SELECT Inf + (@SUBJ) (NEGATE *-1 Inf BARRIER CLB)
           (NEGATE *1 Inf BARRIER CLB)
           (*1 Suutelisusv OR Emotsioonv OR Sobivusv BARRIER CLB LINK 0C Oeldis) ;
SELECT Inf + (@SUBJ) (NEGATE *-1 Inf BARRIER CLB)
               (NEGATE *1 Inf BARRIER CLB)
               (*-1 Vajadusv BARRIER CLB OR SPNom LINK 0C Oeldis) ;
# et vältida: tuli mõte tegema hakata
SELECT Inf + (@SUBJ) (NEGATE *-1 Inf BARRIER CLB)
               (NEGATE *1 Inf BARRIER CLB)
               (*1 Vajadusv BARRIER CLB LINK 0C Oeldis) ;

SELECT Inf + (@SUBJ) (*-1 Vaja BARRIER CLBC) ;
SELECT Inf + (@SUBJ) (*1 Vaja BARRIER CLBC) ;

# da-infinitiiv on subjekt, kui osalauses: öeldiseks on "olema" (üksinda),
# leidub predikatiiv ja puudub nimi- või asesõnaline nominatiivne subjekt
SELECT Inf + (@SUBJ) (*-1C Olema + Main + FinV BARRIER CLB)
               (*-1C (@PRD) BARRIER CLB)
               (NEGATE *-1 SPNom BARRIER CLB)
               (NEGATE *1 SPNom BARRIER CLB) ;
#... aga voimatu oli seda täiesti välja lülitada
#... muidugi oli täiesti voimatu ütelda

# da-infinitiiv on subjekt, kui osalauses esineb järgmine konstruktsioon:
# eelmine sõna on predikatiiv nominatiivis, üleelmine on "olema" ning
# osalauses vasakul ei esine muid sõnu nominatiivis ega partitiivis

SELECT Inf + (@SUBJ) (-2 Olema)(-1C (@PRD))(-1 Nom)
               (NEGATE *-2 Nom BARRIER CLB)(NEGATE *-1 Par BARRIER CLB) ;
# Mul on raske uskuda vrs Laud on raske tosta
# da-infinitiiv pole subjekt, kui lauses leiduvad nii nimi- voi asesona nimetavas kui osastavas käändes
REMOVE Inf + (@SUBJ) (*-1 SP + SgNom BARRIER CLB OR Verb LINK *1C SP + Par) ;

# Nimetavas käändes sona on subjekt, kui lauses on ainult üks nominatiiv ja verb ei ole impersonaal jms.
SELECT Nom + (@SUBJ) (NEGATE 0 N OR Verb)
               (NEGATE *-1 Koma OR ("ja") OR ("ning") OR SPNom OR 
                        Inf + (@SUBJ) OR Imps OR Imper OR Vps1 OR Vps2)
               (NEGATE *1 Koma OR SPNom OR 
                        Inf + (@SUBJ) OR Imps OR Imper OR Vps1 OR Vps2)
               (NEGATE *1 Ncard + Nom BARRIER CLB LINK 1 SgPar LINK NOT 0 Aeg)
               (NEGATE 1 Inf) ;
SELECT Nom + (@SUBJ) (NEGATE 0 N)(*0 Verb BARRIER CLB)
               (NEGATE *-1 Koma OR ("ja") OR ("ning") OR SPNom OR 
                        Inf + (@SUBJ) OR Imps OR Imper OR Vps1 OR Vps2)
               (NEGATE *1 Koma OR SPNom OR 
                        Inf + (@SUBJ) OR Imps OR Imper OR Vps1 OR Vps2)
               (NEGATE *1 Ncard + Nom BARRIER CLB LINK 1 SgPar LINK NOT 0 Aeg)
               (NEGATE 1 Inf) ;
# Viga: Või siis kusagilt tihnikust ohvrile vibunool kõrri lasta
SELECT Nom + (@SUBJ) (NEGATE 0 N OR Nimi)
         (NEGATE *-1 Koma OR Cpr OR ("ja") OR ("ning") OR SPNom OR 
         Inf + (@SUBJ) OR Imps OR Imper OR Vps1 OR Vps2 BARRIER CLB) 
         (NEGATE *1 SPNom OR Inf + (@SUBJ) OR Imps OR Imper OR 
                         Vps1 OR Vps2 BARRIER CLB)
         (*1 Koma BARRIER SP OR CLB)(*-1 FinV BARRIER CLB)
         (NEGATE -1 Partic)(NEGATE 1 Inf) ; 
SELECT (@SUBJ) + Sprop + Nom (NEGATE 1 Nimi)
               (NEGATE *-1 Koma OR ("ja") OR ("ning") OR SPNom OR 
               Inf + (@SUBJ) OR Imps OR Imper OR Vps1 OR Vps2 BARRIER CLB)
               (NEGATE *1 SPNom OR Inf + (@SUBJ) OR Imps OR Imper OR 
                         Vps1 OR Vps2 BARRIER CLB)
               (*1 Koma BARRIER SP OR CLB)(*-1 FinV BARRIER CLB)
               (NEGATE -1 Partic)(NEGATE 1 Inf) ;      
# vaid on unustanud _kõik_ selle


SELECT Nom + (@SUBJ) (NEGATE 0 N)(*1 Koma BARRIER CLBC LINK NOT *1 SP BARRIER CLBC)
               (NEGATE *-1 Koma OR Ja OR SPNom OR Inf + (@SUBJ) OR Imps OR Imper
		 OR Vps12 BARRIER CLBC)
               (NEGATE *1 SPNom OR Inf + (@SUBJ) OR Imps OR Imper OR Vps12
                   BARRIER CLBC)
               (*1 (@FMV) OR (@FCV) BARRIER CLBC)(NEGATE 1 Inf) ;
SELECT SgNom + (@SUBJ) (1C Vps3sg)
    (NEGATE *-1 Koma OR Ja OR SPNom OR Inf + (@SUBJ) OR Imps OR Imper OR Vps12)   
    (NEGATE *1 Koma OR SPNom OR Inf + (@SUBJ) OR Imps OR Imper OR Vps12) ;

SELECT S + (@SUBJ) + Nom 
   (NEGATE *-1 Ja OR SPNom OR Inf + (@SUBJ) OR Imps OR Imper OR Vps12 BARRIER CLBC)
   (NEGATE *1 SPNom OR Inf + (@SUBJ) OR Imps OR Imper OR Vps12 BARRIER CLBC)
   (*-1 (@FMV) ^ Prverb OR (@FCV) ^ Prverb BARRIER CLBC)
   (NEGATE 1 Inf)(NEGATE -1 Võrdlus)  ;

#Vead:
#Sinu numbrid on juba nii mustad, et ajavad juuksed peas püsti.
#Lennartil läheb punast marlborot sigaret sigareti järel
#Kas ei saaks tund aega
SELECT S + (@SUBJ) + Nom 
               (NEGATE *-1 Ja OR SPNom OR Inf + (@SUBJ) OR Imps OR Imper OR
		 Vps12 BARRIER CLB)
               (NEGATE *1 SPNom OR Inf + (@SUBJ) OR Imps OR Imper OR Vps12
		 BARRIER CLB)
               (*1 (@FMV) OR (@FCV) BARRIER CLB)(NEGATE 1 Inf) ;

# Viga: Sõna Ingsots kord ilmus, kord kadus.

SELECT Nom + (@SUBJ) (NEGATE 0 (@ADVL))(NEGATE 1 Nimi)
               (*-1 FinV ^ Imps BARRIER CLB LINK NOT -1 J)
               (NEGATE *-1 Subjekt OR Tud OR Imper OR Inf BARRIER CLB)
               (NEGATE *1 Subjekt OR Tud OR Imper OR Inf BARRIER CLB) ;

#Viga: Viisid minema kõik kuld- ja hõberiistad.

# Kvantoradverb on subjekt, kui pole nimetavas käändes nimi- ega asesonu ega
# subjekti olemasolu välistavaid verbivorme.


SELECT (@SUBJ) (0 KvAdv)(1C (@<Q))
               (NEGATE *-1 Koma OR SPNomPar OR Inf + (@SUBJ) OR Imps OR Imper OR Ja)
               (NEGATE *1 Koma OR SPNomPar OR Inf + (@SUBJ) OR Imps OR Imper) ;
SELECT (@SUBJ) (0 KvPron + NomPar)(1C (@<Q))
               (NEGATE *-1 Koma OR SPNomPar OR Inf + (@SUBJ) OR Imps OR Imper OR Ja)
               (NEGATE *1 Koma OR SPNomPar OR Inf + (@SUBJ) OR Imps OR Imper) ;

SELECT Nom + (@SUBJ) (NEGATE 0 N)
               (NEGATE *-1 Koma OR SPNomPar OR Subjekt OR Imps OR Imper OR
                Ja OR Vps12)
               (NEGATE *1 Koma OR SPNomPar OR Subjekt OR Imps OR Imper OR Vps12)
               (NEGATE 1 Inf) ;
 
SELECT Nom + (@SUBJ) (-1 Neg)(1 Partic) ;

SELECT PlNom + (@SUBJ) (1 Vps3pl) (*1 Supiin + In BARRIER CLB) ;
SELECT ("see" @SUBJ) + Nom (1 Olema)(*1 SPNom BARRIER CLBC) ;
SELECT ("kes" @SUBJ) (-1 Koma)(NEGATE *1 Imps) ;

# Arvsõnad
# N nimetavas käändes on subjekt, kui verb on mitmuse 3. pöördes ja osalauses
# pole mitmuse nimetavat käänet.
SELECT N + (@SUBJ) + Nom (NEGATE 0 ("üks"))(*-1C Vps3pl BARRIER CLB)
                  (NEGATE *-1 PlNom OR Imps BARRIER CLB)
                  (NEGATE *1 PlNom OR Imps BARRIER CLB) ;
SELECT N + (@SUBJ) + Nom (NEGATE 0 ("üks"))(*1C Vps3pl BARRIER CLB)
                  (NEGATE *-1 PlNom OR Imps BARRIER CLB)
                  (NEGATE *1 PlNom OR Imps BARRIER CLB) ;
# 5) Koordinatsioon
# kui sõnade "nii" ja "kui ka" abil koordinatsioonis olevatest sõnadest üks
# ei ole subjekt, siis ei ole seda ka teine
REMOVE (@SUBJ) (-1 Nii)(1 ("kui"))(2 Ka)(NEGATE 3 Subjekt)(4 Verb) ;
REMOVE (@SUBJ) (-4 Nii)(NEGATE -3 Subjekt)(-2 ("kui"))(-1 Ka)(1 Verb) ;
# kui mitmuse partitiivis nimi- või asesõna on on koordineeritud (sidesõnaga)
# samas vormis ühese (kindla) subjektiga, siis ta on ka subjekt
SELECT (@SUBJ) (-2 SP + PlPar)(-2C Subjekt)
               (-1 JaVoi)(0 SP + PlPar)(1 CLB) ;
# kui fraas on tüüpi "suured apelsinid, väikesed õunad" ja "apelsinid" on
# subjekt, siis ka "õunad on subjekt"
SELECT (@SUBJ) (-4 A + PlNom)(-3 S + PlNom)(-2 Koma)
               (-1 A + PlNom)(0 S + PlNom)(-3C Subjekt) ;
SELECT S + (@SUBJ) + PlNom (-1 Adj)(-2 JaVoiK)(-3 PlNom)(-3C Subjekt) ;
SELECT S + (@SUBJ) + PlNom (-1 Eestäiend)(-2 Eestäiend)(-3 JaVoiK)
               (-4 PlNom)(-4C Subjekt) ;
SELECT PlNom + (@SUBJ) (NEGATE 0 Pdet)(-1 JaVoi ^ CLB)(-2 PlNom)(-2C Subjekt) ;
SELECT PlNom + (@SUBJ) (1 JaVoi ^ CLB)(2 PlNom)(2C Subjekt)  ;
SELECT SgNom + (@SUBJ) (-1 JaVoi ^ CLB)(-2 SgNom)(-2C Subjekt)  ;
SELECT SgNom + (@SUBJ) (1 JaVoi ^ CLB)(2 SgNom)(2C Subjekt)  ;

#6) Määrsonalised kvantorid
# määrsõnaline kvantor ei saa olla subjekt, kui paremas kontekstis
# ei leidu kvantori võimalikku laiendit partitiivis
REMOVE Adv + (@SUBJ) (NEGATE 1 Adv OR Par) ;
REMOVE Adv + (@SUBJ) (1 Adv)(NEGATE 2 Par) ;

#7) Asesona ise
REMOVE SP + (@SUBJ) (NEGATE 0 Nom OR Par) ;
REMOVE (@SUBJ) (0 Ise) ;

#8) Sona on sulgudes
SELECT Y + (@SUBJ) (-1 Opr)(1 Cpr)(-2C Subjekt) ;
REMOVE Y + (@SUBJ) (-1 Opr)(1 Cpr)(NEGATE -2 Subjekt) ;
# kui sõna taga on seda tähendav lühend, siis loetakse et nad on
# koordinatsioonis nagu ehk
REMOVE (@SUBJ) (*-1 Opr BARRIER Cpr OR FinV LINK *1 SP BARRIER Cpr)
               (*1 Cpr BARRIER Opr OR FinV) ;
REMOVE (@SUBJ) (*-1 Opr BARRIER Cpr OR FinV )
               (*1 Cpr BARRIER Opr OR FinV LINK *-1 SP BARRIER Opr) ;

# kuid sona ei tohi olla seal üksi
# need on enamasti järellisandid

# III Objektigrammatika

# 1) Teatud liiki öeldised pohjustavad da-infintiivse objekti
SELECT Inf + (@OBJ) (*-1 InfP ^ Inf BARRIER CLB LINK NOT 1 JaVoi) ;
SELECT Inf + (@OBJ) (*1 InfP BARRIER Koma OR CLB) ;
SELECT Inf + (@OBJ) (1 NudTud) ; # oli: (1 (A partic past)) ;
REMOVE Inf + (@OBJ) (NEGATE *-1 InfP BARRIER CLBC)
              (NEGATE *1 InfP BARRIER CLB) ;

# 2) öeldise vorm voi liik välistab objekti
# a) öeldis on intransitiivne
REMOVE (@OBJ) (NEGATE *-1 Verb BARRIER CLB)(NEGATE 1 Partic)
              (*1C Intr&YV BARRIER Verb ^ Intr&YV ^ EiAra OR CLB 
                   LINK NOT *1 Verb BARRIER CLBC) ;

REMOVE (@OBJ) (NEGATE *1 Verb BARRIER CLB)(NEGATE 1 Partic)
              (*-1C Intr&YV BARRIER Verb ^ Intr&YV ^ EiAra OR CLB 
                    LINK NOT *-1 Verb BARRIER CLB) ; 

# Vead: 
# Nagu võiks mõnda teist inimest, kes ei ole nii arukas, sellega ära petta.
# Ta tegi, ..., esikupeegli ees ... grimassi
# Ta võttis pakist, kus oli ..., ühe sigareti.

REMOVE (@OBJ) (NEGATE *1 Verb BARRIER CLB)(NEGATE 1 Partic)
              (*-1C Intr&YV BARRIER Verb ^ Intr&YV ^ EiAra OR CLB 
                    LINK -1 Ei LINK NOT *-1 Verb BARRIER CLB) ; 

REMOVE (@OBJ) (NEGATE 1 Partic)
              (*-1C Intr&YV BARRIER Verb ^ Intr&YV ^ EiAra OR CLB 
                    LINK NOT *-1 Verb BARRIER CLB)
              (*1C Intr&YV BARRIER Verb OR CLB LINK NOT *1 Verb BARRIER CLB) ; 

REMOVE (@OBJ)(0 SgNom)(*1C Intr&YV + Main BARRIER Verb ^ Intr&YV ^ EiAra OR CLBC)
             (NEGATE *1 Partic OR Inf BARRIER CLBC)(NEGATE *-1 Verb BARRIER CLBC) ;

# see oli olnud ...
REMOVE (@OBJ) (NEGATE *-1 Verb BARRIER CLB)
              (*1C Intr&YV BARRIER CLB OR Verb ^ Intr&YV ^ EiAra 
                   LINK *1C Intr&YV BARRIER CLB LINK NOT *1 Verb BARRIER CLB) ;
# Viga: Sammu sõitma hakkasime alles siis, ...

#oli see olnud ...
REMOVE (@OBJ) (*-1C Intr&YV BARRIER CLB LINK NOT *-1 Verb BARRIER CLB)
              (*1C Intr&YV BARRIER CLB LINK NOT *1 Verb BARRIER CLB) ;
REMOVE (@OBJ) (*-1C Intr&YV BARRIER CLB LINK -1  Ei LINK NOT *-1 Verb BARRIER CLB)
              (*1C Intr&YV BARRIER CLB LINK NOT *1 Verb BARRIER CLB) ;
# ei olnud säilinud midagi
# jäi korraks õhku rippuma nagu porikärbes
REMOVE (@OBJ) (*-1C Intr&YV BARRIER CLB OR Verb ^ Intr&YV ^ EiAra
                    LINK *-1C Intr&YV BARRIER CLB 
                    LINK NOT *-1 Verb BARRIER CLB)
              (NEGATE *1 Verb BARRIER CLB) ;

REMOVE (@OBJ) (*-1C Intr&YV BARRIER CLB
                    LINK *-1C Intr&YV BARRIER CLB
                    LINK -1 Ei 
                    LINK NOT *-1 Verb BARRIER CLB)
              (NEGATE *1 Verb BARRIER CLB) ;
REMOVE (@OBJ) (*-1C Intr&YV BARRIER CLB OR Verb ^ Intr&YV ^ EiAra
                    LINK *-1C Intr&YV BARRIER CLB OR Verb ^ Intr&YV ^ EiAra
                    LINK NOT *-1 Verb BARRIER CLB)
              (*1 Kvm BARRIER Verb OR CLB) ;

#b) öeldis on eitavas kones

REMOVE (@OBJ) (0 NimOm)(NEGATE *1 Kysi OR Tud BARRIER CLB)
              (*-1 Ei BARRIER CLB)
              (NEGATE *-1 Tud BARRIER CLB) ;
REMOVE (@OBJ) (0 NimOm)(NEGATE *1 Kysi OR Tud BARRIER CLB)
              (*1 Ei BARRIER CLB)
              (NEGATE *-1 Tud BARRIER CLB) ;
REMOVE Gen + (@OBJ) (NEGATE *1 Kysi BARRIER CLB)(*-1 Ei BARRIER CLB) ;
REMOVE Gen + (@OBJ) (NEGATE *1 Kysi BARRIER CLB)(*1 Ei BARRIER CLB) ;

#c) Isikuline tegumood välistab ainsuse nominatiivse objekti
REMOVE SgNom + (@OBJ) (NEGATE *-1 Imps OR Imper OR InfSupiin BARRIER CLBC)
              (NEGATE *1 Imps OR Imper OR InfSupiin BARRIER CLBC)
              (NEGATE 0 N OR Mis OR Koik) ;
# Viga: Palun see suurem sealt

#d) Umbisikuline tegumood voi käskiv koneviis välistab genitiivse objekti
REMOVE Gen + (@OBJ) (*-1C Imps BARRIER CLBC) ;
REMOVE Gen + (@OBJ) (*1C Imps BARRIER CLBC) ;
REMOVE Gen + (@OBJ) (*-1C Imper BARRIER CLBC) ;
REMOVE Gen + (@OBJ) (*1C Imper BARRIER CLBC) ;

#e) Verb nouab partitiivset objekti

SELECT (@OBJ) (*-1 PartObjVerb BARRIER CLBC)
              (0 Par ^ Aeg)
              (NEGATE *-1 Par + (@OBJ) OR Ära BARRIER CLBC )
              (NEGATE *1 Par + (@OBJ) OR Ära BARRIER CLBC) ;
#Viga: Kui ülempreester ei tahtnud loomulikult _sellist_ rahakat ning -...

SELECT (@OBJ) (*1 PartObjVerb BARRIER CLBC)
              (0 Par ^ Aeg)
              (NEGATE *-1 Par + (@OBJ) OR Ära BARRIER CLBC )
              (NEGATE *1 Par + (@OBJ) OR Ära BARRIER CLBC) ;

REMOVE (@OBJ) (NEGATE 0 Par OR Inf OR Pl)
              (*-1 PartObjVerb + Ps BARRIER CLB OR Verb ^ PartObjVerb 
                   LINK NOT *-1 Verb BARRIER CLB)
              (NEGATE *1 Verb BARRIER CLB)
              (NEGATE *-1 Tr OR Ära BARRIER CLB)(NEGATE *1 Tr OR Ära BARRIER CLB)
              (NEGATE 1 Partic)  ;

REMOVE (@OBJ) (NEGATE 0 Par OR Inf OR Pl)
              (*1 PartObjVerb + Ps BARRIER CLB OR Verb ^ PartObjVerb 
                   LINK NOT *1 Verb BARRIER CLB)
              (NEGATE *-1 Verb BARRIER CLB)
              (NEGATE *-1 Tr OR Ära BARRIER CLB)(NEGATE *1 Tr OR Ära BARRIER CLB)
              (NEGATE 1 Partic)  ;

#3) Erijuhud

# Kui objekt ja subjekt on samas vormis, siis tavaliselt subjekt eelneb objektile
REMOVE (@OBJ) (0 (@SUBJ) + PlNom)
              (NEGATE *-1 Subjekt BARRIER CLB)
              (*1 FinV BARRIER CLB LINK *1 (@OBJ) + PlNom BARRIER Sid|Kvm OR CLB) ;
# Esimeses kahes isikus isikulised asesonad ei ole nom objektid 
REMOVE (@OBJ) (0 Ppers + Pps1 + Nom OR Ppers + Pps2 + Nom) ;
# kui osalauses leidub kaks sona, mis voivad olla nii objektiks kui ka
# subjektiks, üks nominatiivne ja teine partitiivne, siis nominatiivne ei ole
# objekt

REMOVE (@OBJ) (0 (@SUBJ) + Nom)(NEGATE 1 Partic)
              (NEGATE *-1 InfSupiin BARRIER CLB)
	      (NEGATE *-1 InfSupiin BARRIER CLB)
              (*1 (@OBJ) + Par ^ AtrAdv BARRIER CLB) ;

# kombinatsioon Obj(nom/gen) Fv SubjNom ei ole võimalik

REMOVE Gen + (@OBJ) (NEGATE 1 Oeldis OR CLB)
              (NEGATE *-1 Oeldis OR NudTud BARRIER CLB)
              (*2C Oeldis BARRIER CLB LINK *1 SPNom BARRIER Kvm OR CLB) ;

REMOVE SgNom + (@OBJ) (NEGATE 0 Mis)(NEGATE *1 Ise BARRIER CLB)
              (NEGATE *-1 Oeldis BARRIER CLB)
              (*1C Oeldis BARRIER CLB LINK *1 SPNom BARRIER Sid|Kvm OR CLB) ;
# On voimalik: ta sai aru, kui suure _asja_ ta on ette votnud
# Juba tempel ise on püstitatud ...

#Arvsõnad ei ole genitiivsed objektid
REMOVE N + Gen + (@OBJ) (NEGATE 0 ("üks")) ;
#Tud-vormile ei eelne gen objekti
REMOVE Gen + (@OBJ) (1 Tud) ;
#      Nt tema lukustatud kabinetis


#Kui nud-, tud- v_i des-vormile eelneb vahetult partitiiv , siit ta on objekt
SELECT (@OBJ) (0 Par)(1 NudTudDes ^ Intr&YV)(NEGATE 0 (@<P) OR Aeg OR Pdem)
              (NEGATE *-1 Neg BARRIER CLB) ;

SELECT Pdem + Par + (@OBJ) (1 NudTudDes ^ Intr&YV)(NEGATE 0 (@<P) OR Aeg)
              (NEGATE *-1 Neg BARRIER CLB)(*1 NPpiir BARRIER Par OR CLB) ;

# Osastavas käändes sõna enne partitsiipi on suure tõenäosusega sihitis,
# v.a. määrused
SELECT S + Par + (@OBJ) (NEGATE 0 (@ADVL))(1 Partic ^ Intr&YV) ;

# Kui lauses pole verbi, ei ole ka nominatiivset objekti
REMOVE Nom + (@OBJ) (NEGATE *1 Verb)(NEGATE *-1 Verb) ;

#Kaassonafraasis ei ole objekti
REMOVE (@OBJ) (*-1 Kpre BARRIER (@<P) OR CLB)
              (*1 (@<P) BARRIER Kpre OR CLB) ;
# Kui omastav ja osastav kääne on korvuti ja molemad voivad olla objektid, siis
# omastav ei ole objekt, v.a erandid
REMOVE Gen + (@OBJ) (1 (@OBJ) + Par)(NEGATE 1 (@Q>) OR (@<P) OR (@P>))
              (NEGATE 2 Partic OR InfSupiin) ;
# Viga: Ta võttis südame rindu

REMOVE ("oma" @OBJ) (1 (@OBJ) + Par)(NEGATE 2 Partic OR InfSupiin) ;
REMOVE Gen + (@OBJ) (1 A + Par)(2 (@OBJ) + Par)(NEGATE 3 Partic OR InfSupiin) ;
REMOVE ("oma" @OBJ) (1 A + Par)(2 (@OBJ) + Par)(NEGATE 3 Partic OR InfSupiin) ;

#4) Unikaalsus
# kui osalauses ei leidu partitsiipe, gerundiive, supiine ega infinitiive
# ning leidub kindel objekt, kusjuures ei leidu rinnastavaid komasid ega
# sidesonu,  siis osalauses ei saa olla rohkem objekte


REMOVE (@OBJ)(NEGATE 0 Inf)
             (NEGATE *1 Partic OR SupInfGer BARRIER CLB)
             (NEGATE *-1 Partic OR SupInfGer BARRIER CLB)
             (*1C Objekt BARRIER Sid|Kvm OR CLB)  ;
#Viga tekkis siis, kui eelnevalt valesti määratud:
#Laseks ta nooli täis nagu Püha Sebastianuse
REMOVE (@OBJ)(NEGATE 0 Inf)
             (NEGATE *1 Partic OR SupInfGer BARRIER CLB)
             (NEGATE *-1 Partic OR SupInfGer BARRIER CLB)
             (*-1C Objekt BARRIER Sid|Kvm OR CLB)  ;

#või vaeglauses
#nemad hoidsid teda ka nagu minu vanemad mind
#pani ka noor kirjanik nõukogudemaad kiitvaid lauseid paberile

#riik sai miljardeid kroone kahjumit 



REMOVE (@OBJ)(*-1 Aux BARRIER CLB)
             (*1 Partic BARRIER CLB)
             (*1C Objekt BARRIER Sid|Kvm OR CLB)
             (NEGATE *-1 SupInfGer BARRIER CLB)(NEGATE *1 SupInfGer BARRIER CLB) ;
REMOVE (@OBJ)(*-1 Aux BARRIER CLB)
             (*-1 Partic BARRIER CLB)
             (*-1C Objekt BARRIER Sid|Kvm OR CLB)
             (NEGATE *-1 SupInfGer BARRIER CLB)(NEGATE *1 SupInfGer BARRIER CLB) ;
REMOVE (@OBJ)(*1 Aux BARRIER CLB)
             (*1 Partic BARRIER CLB)
             (*1C Objekt BARRIER Sid|Kvm OR CLB)
             (NEGATE *-1 SupInfGer BARRIER CLB)(NEGATE *1 SupInfGer BARRIER CLB) ;

# 5) Koordinatsioon
# kui koordinatsioonis olevatest samas vormis sonadest üks ei ole objekt,
# siis ei ole seda ka teine
REMOVE (@OBJ) (-1 Nii)(1 Kui)(2 Ka)(NEGATE 3 Objekt)(4 Verb) ;
REMOVE (@OBJ) (-4 Nii)(NEGATE -3 Objekt)(-2 Kui)(-1 Ka)(1 Verb) ;

REMOVE SP + SgNom + (@OBJ) (1 JaVoi ^ CLBC)
              (2 SP + SgNom)(NEGATE 2 Objekt OR Eestäiend) ;
REMOVE SP + PlNom + (@OBJ) (1 JaVoi ^ CLBC)
              (2 SP + PlNom)(NEGATE 2 Objekt OR Eestäiend) ;
REMOVE SP + SgGen + (@OBJ) (1 JaVoi ^ CLBC)
              (2 SP + SgGen)(NEGATE 2 Objekt OR Eestäiend) ;
REMOVE SP + SgPar + (@OBJ) (1 JaVoi ^ CLBC)
              (2 SP + SgPar)(NEGATE 2 Objekt OR Eestäiend) ;
REMOVE SP + PlPar + (@OBJ) (1 JaVoi ^ CLBC)
              (2 SP + PlPar)(NEGATE 2 Objekt OR Eestäiend) ;
REMOVE SP + SgNom + (@OBJ) (-1 JaVoi ^ CLBC)
              (-2 SP + SgNom)(NEGATE -2 Objekt) ;
REMOVE SP + PlNom + (@OBJ) (-1 JaVoi ^ CLBC)
              (-2 SP + PlNom)(NEGATE -2 Objekt) ;
REMOVE SP + SgGen + (@OBJ) (-1 JaVoi ^ CLBC)
              (-2 SP + SgGen)(NEGATE -2 Objekt) ;
REMOVE SP + SgPar + (@OBJ) (-1 JaVoi ^ CLBC)
              (-2 SP + SgPar)(NEGATE -2 Objekt) ;
REMOVE SP + PlPar + (@OBJ) (-1 JaVoi ^ CLBC)
              (-2 SP + PlPar)(NEGATE -2 Objekt) ;

SELECT SP + SgNom + (@OBJ) (1 JaVoi ^ CLBC)
              (2 SP + SgNom)(2C Objekt) ;
SELECT SP + PlNom + (@OBJ) (1 JaVoi ^ CLBC)
              (2 SP + PlNom)(2C Objekt) ;
SELECT SP + SgGen + (@OBJ) (1 JaVoi ^ CLBC)
              (2 SP + SgGen)(2C Objekt) ;
SELECT SP + SgPar + (@OBJ) (1 JaVoi ^ CLBC)
              (2 SP + SgPar)(2C Objekt) ;
SELECT SP + PlPar + (@OBJ) (1 JaVoi ^ CLBC)
              (2 SP + PlPar)(2C Objekt) ;
SELECT SP + SgNom + (@OBJ) (-1 JaVoi ^ CLBC)
              (-2 SP + SgNom)(-2C Objekt) ;
SELECT SP + PlNom + (@OBJ) (-1 JaVoi ^ CLBC)
              (-2 SP + PlNom)(-2C Objekt) ;
SELECT SP + SgGen + (@OBJ) (-1 JaVoi ^ CLBC)
              (-2 SP + SgGen)(-2C Objekt) ;
SELECT SP + SgPar + (@OBJ) (-1 JaVoi ^ CLBC)
