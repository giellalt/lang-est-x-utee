             (NEGATE 1 Hiljem)
             (NEGATE -1 Iga OR JärgArv)
             (NEGATE *-1 Terve OR Võrdlus BARRIER CLB OR FV) ;
#Vead: 
# Ta vottis istet samas reas kus _Winstongi_
# päevadel, mis ma abahakiiride juures oline
# Mis see meie asi on (2x)
# läheme jala ja kõik
# kõiki oleks tulnud kõnetada seltsimees

REMOVE Nom + (@ADVL) (0 SP)(NEGATE 0 Aeg OR Arvsõna OR NimKv OR Lauselaiend)
             (NEGATE 1 Hiljem)
             (NEGATE -1 Iga OR JärgArv)
             (*-1 Võrdlus + CLB BARRIER CLB OR FV) ;
REMOVE Nom + (@ADVL) (0 SP)(NEGATE 0 Aeg OR Arvsõna OR NimKv OR Lauselaiend)
             (NEGATE 1 Hiljem)
             (NEGATE -1 Iga OR JärgArv)
             (*-1 Võrdlus + Kui BARRIER CLB OR FV LINK 1 Ka) ;

REMOVE Nom + (@ADVL) (0 SP)(NEGATE 0 Aeg OR Arvsõna OR NimKv OR Lauselaiend)
             (NEGATE 1 Hiljem)
             (NEGATE -1 Iga OR JärgArv)
             (*-1 Võrdlus BARRIER CLB OR FV OR JaVoi LINK 1 SPNom LINK NOT 0 (@NN>)) ;


REMOVE Par + (@ADVL) (NEGATE -1 JärgArv)(NEGATE 0 Tervitus OR NimKv OR ("kord") OR Aeg)
              (NEGATE 0 Arvsõna OR Viisimäärus OR KedaMida OR ("see"))
              (NEGATE 1 Võrre)(NEGATE *-1 Võrdlus BARRIER CLBC) ;
REMOVE Par + (@ADVL) (NEGATE -1 JärgArv)(NEGATE 0 Tervitus OR NimKv OR ("kord") OR Aeg)
              (NEGATE 0 Arvsõna OR Viisimäärus OR KedaMida OR ("see"))
              (NEGATE 1 Võrre)(*-1 Võrdlus + CLB BARRIER CLB) ;

REMOVE ("see" @ADVL) + Par (NEGATE 1 KompKvAdv)(NEGATE 1 Võrre)(NEGATE 1 ("et"))
              (NEGATE *-1 Võrdlus BARRIER CLB) ;
REMOVE Par + (@ADVL) (NEGATE -1 JärgArv)(NEGATE 0 Tervitus OR NimKv OR ("kord") OR Aeg)
              (NEGATE 0 Arvsõna OR Viisimäärus OR KedaMida OR ("see"))
              (NEGATE 1 Võrre)(*-1 Võrdlus BARRIER CLB LINK 1 SPNom) ;

REMOVE Par + (@ADVL)(0 KedaMida)(1C Subjekt) ;
SELECT N + Gen + (@ADVL)(1 Adjühik) ;
SELECT Gen + (@ADVL)(1 SpetsAdj)(1C Adj) ;
SELECT Gen + (@ADVL)(1 JaVoi)(2 S + Gen)(3 SpetsAdj) ;


# Omastava käände korral tuleb jälgida, et järgnev sona
# ei oleks pikkune-tüüpi omadussona

REMOVE Sprop + Gen + (@ADVL) (NEGATE 1 Tud OR Ger OR Inf OR Tav)
               (NEGATE *-1 Võrdlus BARRIER CLB) ;
REMOVE Gen + (@ADVL) (NEGATE -1 JärgArv)(NEGATE 0 NimKv OR ("kord") OR Aeg OR Arvsõna)
              (NEGATE *1 NiNaTaGa BARRIER NPpiir OR CLB OR Side)
              (NEGATE 1 NudTudDes OR Inf OR Tav)(NEGATE *-1 Võrdlus BARRIER CLB) ;

# Mitmuses omastavas käändes nimisona pole määrus, kui ei järgne pikune-tüüpi omadussona
REMOVE PlGen + (@ADVL)(NEGATE 1 Sid|Kvm)(NEGATE 1 NudTudDes)(NEGATE 1 Inf)(NEGATE 1 Tav) ;
REMOVE PlGen + (@ADVL)(1 Sid|Kvm)(NEGATE *1 NiNaTaGa BARRIER CLB) ;

# Nimisonalised kvantorid ei ole määrused:
REMOVE Nom + (@ADVL)(0 NimKv)(NEGATE 0 Aeg)(NEGATE 1 Hiljem)
                (NEGATE -1 Iga)(NEGATE -1 JärgArv)
                (NEGATE *-1 Võrdlus BARRIER CLBC)(NEGATE *1 JärelKv BARRIER CLBC) ;

# Üksikud juhud:
REMOVE ("miski" @ADVL) + Par ;
REMOVE ("oma" @ADVL) (0 NGP)(NEGATE 1 Tav)(NEGATE 1 Tud)(NEGATE 1 Inf) ;


# Ma-tegevusnimi on määrus, kui osalauses leidub vastav verb
SELECT Supiin + (@ADVL) (*-1C FV + SupIllAdvVerb BARRIER Sid|Kvm OR CLB) ;

# Lauselaiendid ja tervitused:
REMOVE A + Nom + (@ADVL) (NEGATE 0 Lauselaiend) ;
SELECT (@ADVL) (0 Lauselaiend)(-1 <<<)(1 Koma) ;
SELECT (@ADVL) (0 Lauselaiend)(-2 <<<)(-1 ("väga"))(1 Koma) ;
SELECT (@ADVL) (0 Lauselaiend)(-1 <<<)(1 Küll) ;
SELECT (@ADVL) (0 Lauselaiend)(-1 Kvm)(1 Küll) ;
REMOVE A + Nom + (@ADVL) (0 Lauselaiend)(NEGATE 1 Koma) ;


# nimisona seestütlevas on määrus,  kui järgneb keskvorre
SELECT El + (@ADVL) (*1 Võrre BARRIER FV OR CLB) ;
SELECT Term + (@ADVL) (1 Täis) ;
# omadussona neljas viimases käändes on määrus
SELECT (@ADVL) (0 Adj + NiNaTaGa) ;
SELECT A + Gen + (@ADVL) (1 JaVoi)(2 Adj + NiNaTaGa) ;
# omadussona omastavas pole määrus, kui paremal ei ole neljas viimases käändes omadussona.
REMOVE A + Gen + (@ADVL) (NEGATE *1 Adj + NiNaTaGa) ;
# adverb enne partitsiipi on määrus
SELECT Adv + (@ADVL) (1 Partic) ;
SELECT Digit + (@ADVL) (-1 OPar)(1 CPar) ;
REMOVE Digit + (@ADVL) (1 Fst)(*1 S BARRIER CLB) ;
REMOVE Digit + (@ADVL) (1 Fst)(2 ("a") + Y) ; 
# Kui numbrina kirjutatud Nu järel on punkt ja see ei ole lause lopp.

SELECT QuotPresPs + (@ADVL) (NEGATE -1 Tud)(*0 VatAdvVerb BARRIER CLB) ;

AFTER-SECTIONS

# SUBJEKTI HEURISTILISED REEGLID 
# Need reeglid on kas nii uued, et on korralikult testimata, voi on nende tekitatud vigade hulk suur.
REMOVE Inf + (@SUBJ) (NEGATE *-1 TeisedDaInfv OR Emotsioonv OR Sobivusv 
                        OR Vajadusv OR Suutelisusv BARRIER CLBC)
		   (NEGATE *1 TeisedDaInfv OR Emotsioonv OR Sobivusv 
                        OR Vajadusv OR Suutelisusv BARRIER CLBC) ;

# Subjekt eelneb predikatiivile
SELECT S + (@SUBJ) (NEGATE 0 (@OBJ) OR (@ADVL)) (1 FinV)
                (*2 SPNom + (@PRD) BARRIER CLBC) ;

# Nimetavas käändes sõnale järgneb kohe järgmine osalause
# Nt. Mees, kes ....
SELECT Nom + (@SUBJ) (NEGATE 0 N)(1 Koma OR OPar)(NEGATE *-1 Verb BARRIER CLB)
               (NEGATE *1 SP OR Verb BARRIER CLB)(NEGATE *-1 Dsh) ; # TP +Dsh

# Veidi laiendatud unikaalsuse printsiip, mis ei Nesta sidesõnade ja
# kvm-dega, sest leitud alus ja sõna on erinevates käänetes
REMOVE (@SUBJ) (NEGATE 0 Nom)(*-1C Subjekt + Nom)
                  (*-1 FinV BARRIER CLBC LINK NOT *-1 FinV) ;
REMOVE (@SUBJ) (NEGATE 0 Nom)(*1C Subjekt + Nom)
                  (*1 FinV BARRIER CLBC LINK NOT *1 FinV) ;
# Lause alguses on potentsiaalne subjekt, mis on nimetavas käändes,
# sõna ise on osastavas ja paremas kontekstis on infinitiiv.
 REMOVE Par + (@SUBJ) (*1 Inf BARRIER CLB)(*-1 Subjekt + Nom BARRIER CLB) ;
#Kui sõna on mitmuse nominatiivis ja järgneb vahetul verb pl3 ja
#rohkem mitmuse nominatiive osalauses pole
#vigu tekib poolikutes osalausetes. 
SELECT PlNom + (@SUBJ) (1 Vps3pl) (NEGATE *2 SP + PlNom)
                  (NEGATE *-1 Imps BARRIER CLB)
                  (NEGATE *1 Imps BARRIER CLB)
                  (NEGATE *-1 Subjekt BARRIER CLB) ;
# Tüüplause Ad Intr&YVVerb SUBJ
SELECT (@SUBJ) (NEGATE 0 Inf)
                  (*-1 In OR Ad BARRIER CLB 
                       LINK NOT *-1 SP BARRIER CLB)
                  (*-1 In OR Ad BARRIER Subjekt OR CLB LINK 1 Intr&YV)
                  (NEGATE *1 Subjekt BARRIER CLB)
                  (NEGATE *-1 Inf OR Tud BARRIER CLB)
                  (NEGATE *1 Inf OR Tud BARRIER CLB) ;
SELECT (@SUBJ) (NEGATE 0 Inf)(*-1 In OR Ad BARRIER CLB 
                               LINK NOT *-1 SP BARRIER CLB)
                  (*-1 In OR Ad BARRIER SPNom OR CLB LINK 1 Intr&YV)
                  (NEGATE *1 SPNom BARRIER CLB)
                  (NEGATE *-1 Tud BARRIER CLB)
                  (NEGATE *1  Tud BARRIER CLB)(1C (@<INFN)) ;

SELECT (@SUBJ) (*-1 In OR Ad BARRIER CLB LINK NOT *-1 SP BARRIER CLB)
               (*-1 In OR Ad BARRIER CLB OR SPNom LINK 1 Intr&YV)
               (NEGATE *-1 Tud OR Imper BARRIER CLB)
               (NEGATE *1  Tud OR Imper BARRIER CLB)
               (*1C Objekt BARRIER CLB LINK *1 Inf BARRIER CLB) ;

SELECT Nom + (@SUBJ) (NEGATE 0 N)(NEGATE *-1 Koma)(NEGATE *1 Koma)
               (NEGATE *-1 Subjekt)(NEGATE *1 Subjekt)
               (NEGATE 1 Inf)(NEGATE *-1 Imps)(NEGATE *1 Imps)
               (NEGATE *-1 Imper)(NEGATE *1 Imper)(NEGATE *-1 Vps12)(NEGATE *1 Vps12) ;

SELECT Nom + (@SUBJ) (NEGATE 0 Määrus)
               (NEGATE *-1 Koma BARRIER CLB)
               (*1 Koma BARRIER CLB LINK *1 SP BARRIER CLB)
               (NEGATE *-1 Subjekt BARRIER CLB)(NEGATE *1 Subjekt BARRIER CLB)
               (*1 CLB ^ Ja)(*-1 FinV BARRIER CLB)(NEGATE 1 Inf)
               (NEGATE *-1 Imps BARRIER CLB)(NEGATE *1 Imps BARRIER CLB)
               (NEGATE *-1 Imper BARRIER CLB)(NEGATE *1 Imper BARRIER CLB)
               (NEGATE *-1 Vps12 BARRIER CLB)(NEGATE *1 Vps12 BARRIER CLB)
               (NEGATE -1 Partic) ;
# välja jätta või maha tõmmata on lihtne
 SELECT Inf + (@SUBJ) (*1C (@PRD) + Adj)
          (NEGATE *-1 SPNom BARRIER CLB)(NEGATE *1 SPNom BARRIER CLB) ;

#Kui sõna võib olla kvantori laiend ja kvantor ise võib olla alus, siis
#kustuta kvantori märgend
#Laual oli kuhi musti toidunõusid
REMOVE (@SUBJ)(0 JärelKv)(NEGATE 0 Nom)
        (*-1 NimKv + Subjekt OR ArvKv + Subjekt OR KvAdv + Subjekt BARRIER CLB LINK NOT 1 Scom) ;

# Koordinatsioon
SELECT PlNom + (@SUBJ) (NEGATE 0 Pdet)(-1 JaVoiK)(-2 PlNom)(-2C Subjekt) ;
SELECT PlNom + (@SUBJ) (1 JaVoiK)(2 PlNom)(2C Subjekt) ;
SELECT SgNom + (@SUBJ) (-1 JaVoiK)(-2 SgNom)(-2C Subjekt)
               (NEGATE *1 Imps BARRIER CLBC) ;
SELECT SgNom + (@SUBJ) (1 JaVoiK)(2 SgNom)(2C Subjekt) ;
SELECT Nom + (@SUBJ) (*-1 JaVoi BARRIER Head OR CLBC LINK -1C Subjekt + Nom) ;
SELECT Nom + (@SUBJ) (*-1 Koma BARRIER Head LINK -1C Subjekt + Nom)
                      (*1 JaVoi BARRIER CLBC LINK *1 Subjekt BARRIER CLBC) ;

SELECT (@SUBJ) (0 KvAdv)(*-1 Olema BARRIER CLB)
               (NEGATE *-1 Subjekt BARRIER CLB)(NEGATE *1 Subjekt BARRIER CLB) ;

AFTER-SECTIONS

# osalause algab kes-või-mis, millele järgneb verbi sg 3 ja pole
# mineviku partitsiipi


SELECT ("kes" @SUBJ) OR ("mis" @SUBJ) (-1 JaVoiK)(0 Sg)(1 Vps3sg)
                   (NEGATE *1 Imps BARRIER CLB) ;
SELECT ("kes" @SUBJ) OR ("mis" @SUBJ) (-1 JaVoiK)(0 Pl)(1 Vps3pl)
                   (NEGATE *1 Imps BARRIER CLB) ;


REMOVE S + (@SUBJ) (*-1C Inf + (@SUBJ) BARRIER CLB) ;
REMOVE (@SUBJ)   (0 (@PRD))(NEGATE 0 Määrus)(NEGATE 0 Objekt)
                 (*-1 Prverb BARRIER (@PRD) OR NotEestäiend OR CLBC
                  LINK *-1C Subjekt BARRIER (@PRD) OR NotEestäiend OR CLBC) ;
SELECT Ppers + Pl + Pps3 + (@SUBJ) + Nom (*1 Vps3pl BARRIER CLB)  ;
REMOVE S + Par + (@SUBJ) (*-1 FV + ParObjVerb BARRIER CLB)(NEGATE *1 Par BARRIER Kvm OR CLB) ;
REMOVE S + Par + (@SUBJ) (*1 FV + ParObjVerb BARRIER CLB)(NEGATE *1 Par BARRIER Kvm OR CLB) ;
REMOVE S + Par + (@SUBJ) (*-1 FV + Trans BARRIER CLB)(NEGATE *1 Par BARRIER Kvm OR CLB) ;
REMOVE S + Par + (@SUBJ) (*1 FV + Trans BARRIER CLB)(NEGATE *1 Par BARRIER Kvm OR CLB) ;


# uued lisareeglid

END
